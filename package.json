    "@types/pg": "^8.11.14",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "nanoid": "^5.1.5",
    "node-telegram-bot-api": "^0.64.0",
    "pg": "^8.15.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "telegraf": "^4.16.3",
    "typescript": "^5.3.3",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/cors": "^2.8.17",
